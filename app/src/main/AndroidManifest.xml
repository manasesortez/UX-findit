<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.wazunga.findit">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".slide3"
                android:theme="@style/AppTheme_None">
        </activity>
        <activity android:name=".slide2"
                android:theme="@style/AppTheme_None">
        </activity>
        <activity android:name=".slide1"
                android:theme="@style/AppTheme_None">
        </activity>
        <activity
                android:name=".menuCategorias"
                android:theme="@style/AppTheme_every">
        </activity>
        <activity
                android:name=".Welcome"
                android:theme="@style/AppTheme_welcome">
        </activity>
        <activity
                android:name=".MainActivity"
                android:theme="@style/AppTheme_None">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>